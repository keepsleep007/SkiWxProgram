page{background:#f8f8f8;padding-bottom:160rpx}.carousel{width:100%;overflow:hidden;position:relative}.image-wrapper{width:100%;height:500rpx;display:flex}.image-wrapper image{width:100%;height:auto;object-fit:cover}.introduce-section{background:#fff;padding:20rpx 30rpx}.introduce-section .title{font-size:32rpx;color:#303133;height:50rpx;line-height:50rpx}.detail-desc{background:#fff;margin-top:16rpx;margin-left:16rpx;margin-right:16rpx}.detail-desc .d-header{display:flex;justify-content:center;align-items:center;height:80rpx;font-size:30rpx;color:#303133;position:relative}.detail-desc .d-header text{padding:0 20rpx;background:#fff;position:relative;z-index:1}.detail-desc .d-header:after{position:absolute;left:50%;top:50%;-webkit-transform:translateX(-50%);transform:translateX(-50%);width:300rpx;height:0;content:"";border-bottom:1px solid #ccc}